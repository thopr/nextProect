{
  "name": "@bit/zenhomes.zenhomes-ui.checkbox",
  "version": "0.0.3",
  "homepage": "https://bit.dev/zenhomes/zenhomes-ui/checkbox",
  "main": "dist/index.js",
  "componentId": {
    "scope": "zenhomes.zenhomes-ui",
    "name": "checkbox",
    "version": "0.0.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11"
  },
  "devDependencies": {},
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/react-typescript@3.1.52": {
          "rawConfig": {
            "compilerPath": "typescript/bin/tsc",
            "compilerArguments": [
              "--declaration"
            ],
            "compiledFileTypes": [
              ".ts",
              ".tsx"
            ],
            "configFileName": "tsconfig.json",
            "tsconfig": {
              "compilerOptions": {
                "outDir": "dist",
                "target": "ES5",
                "sourceMap": false,
                "moduleResolution": "node",
                "esModuleInterop": true,
                "module": "CommonJS",
                "allowSyntheticDefaultImports": true,
                "resolveJsonModule": true,
                "declaration": true,
                "experimentalDecorators": true,
                "inlineSourceMap": false,
                "inlineSources": false,
                "lib": [
                  "es6",
                  "dom",
                  "es2016",
                  "es2017",
                  "ES2018"
                ],
                "jsx": "react",
                "rootDir": "./",
                "removeComments": true,
                "typeRoots": [
                  "./node_modules/@types"
                ],
                "importHelpers": false,
                "allowJs": false,
                "forceConsistentCasingInFileNames": true,
                "noImplicitReturns": true,
                "noImplicitThis": true,
                "noImplicitAny": true,
                "suppressImplicitAnyIndexErrors": true,
                "noUnusedLocals": true,
                "noUnusedParameters": true
              },
              "include": [
                "./**/*"
              ],
              "exclude": [
                "node_modules",
                ".dependencies",
                "dist"
              ]
            },
            "development": false,
            "copyPolicy": {
              "ignorePatterns": [
                "package.json",
                "package-lock.json",
                "tsconfig.json"
              ],
              "disable": false
            }
          },
          "options": {}
        }
      }
    }
  }
}
